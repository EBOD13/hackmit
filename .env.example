# Environment Variables for Quest HUD App
# Copy this file to .env and fill in your actual values

# Your Mentra OS app package name (get from console.mentra.glass)
PACKAGE_NAME=your-package-name-here

# Your Mentra OS API key (get from console.mentra.glass)
MENTRAOS_API_KEY=your-api-key-here

# Port for the development server
PORT=3000

# Optional: Enable debug logging
DEBUG=true
# 
